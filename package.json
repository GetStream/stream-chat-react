    "@testing-library/react-hooks": "^3.4.1",
    "@types/braintree__sanitize-url": "4.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/emoji-mart": "^3.0.2",
    "@types/linkifyjs": "^2.1.3",
    "@types/lodash.debounce": "^4.0.6",
